{
  "firestore": {
    "database": "(default)",
    "location": "eur3",
    "rules": "rules_version = '2';",
    "indexes": "atch /databases/{database}/documents {"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
